{
  "name": "terrafarm-frontend",
  "private": true,
  "version": "0.3.0-alpha.1",
  "description": "Frontend client and server code for Terrafarm",
  "repository": "rojobuffalo/terrafarm-frontend",
  "author": "rojobuffalo",
  "license": "Creative Commons BY-NC-ND 3.0",
  "keywords": [],
  "main": "gulpfile.babel.js",
  "scripts": {
    "setup": "cp -n .env.example .env || true",
    "schema": "gulp load-schema",
    "relay": "relay-compiler --src ./src --schema ./data/schema.graphql",
    "relay-watch": "relay-compiler --src ./src --schema ./data/schema.graphql --watch",
    "dev-server": "gulp webpack-dev-server",
    "dev": "npm run relay && npm run dev-server",
    "lint": "eslint ./src",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha --opts spec/mocha.opts",
    "build": "rimraf build && npm run relay && WEBPACK_ENV=production gulp build",
    "start": "pm2 start ecosystem.json --env production",
    "restart": "pm2 restart ecosystem.json",
    "stop": "pm2 stop terrafarm-frontend"
  },
  "engines": {
    "node": "6.11.1",
    "npm": "3.10.10"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "deep-equal": "^1.0.1",
    "farce": "^0.2.1",
    "formsy-material-ui": "^0.6.1",
    "formsy-react": "^0.19.5",
    "found": "^0.3.2",
    "found-relay": "^0.3.0-alpha.5",
    "graphql": "^0.10.5",
    "http-proxy": "^1.13.2",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.18.6",
    "prop-types": "^15.5.10",
    "react": "^15.4.0",
    "react-addons-create-fragment": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.0",
    "react-addons-pure-render-mixin": "^15.4.0",
    "react-addons-transition-group": "^15.4.0",
    "react-addons-update": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-ga": "^2.1.2",
    "react-icons": "^2.0.1",
    "react-relay": "^1.1.0",
    "react-tap-event-plugin": "^2.0.1",
    "relay-local-schema": "^0.6.2",
    "relay-runtime": "^1.1.0",
    "sanitize.css": "^5.0.0",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-airbnb": "^2.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.7.1",
    "eslint": "^3.19.0",
    "eslint-config-terrafarm-react": "^0.2.2",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.2",
    "fetch-everywhere": "^1.0.5",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.0",
    "gulp-env": "^0.4.0",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.28.0",
    "jsonwebtoken": "^7.3.0",
    "loglevel": "^1.4.1",
    "mocha": "^3.4.2",
    "mock-local-storage": "^1.0.3",
    "node-fetch": "^1.6.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "relay-compiler": "^1.1.0",
    "relay-compiler-webpack-plugin": "^0.3.0",
    "require-hacker": "^3.0.0",
    "sinon": "^2.3.8",
    "source-map-support": "^0.4.12",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.4.2"
  }
}
